{
  "name": "@apy-finance/apy-core",
  "version": "1.0.0",
  "description": "",
  "keywords": [
    "ethereum",
    "solidity",
    "defi"
  ],
  "files": [
    "contracts",
    "artifacts",
    "config"
  ],
  "scripts": {
    "check": "npm run lint",
    "clean": "buidler clean",
    "compile": "buidler compile",
    "test": "npx buidler test",
    "lint": "npx solhint contracts/**/*.sol",
    "ganache": "ganache-cli",
    "fork:mainnet": "ganache-cli -u \"$(node get_unlocked_addresses.js)\" -f $(node get_infura.js) -m \"$(node get_mnemonic.js)\" ",
    "integration": "npx buidler --network ganache test ./integration/*.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "./git_hooks/commit-msg"
    }
  },
  "lint-staged": {
    "contracts/**/*.sol": "solhint -w 0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/apy-finance/apy-core.git"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "author": "APY.Finance",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/apy-finance/apy-core/issues"
  },
  "homepage": "https://github.com/apy-finance/apy-core/README.md",
  "devDependencies": {
    "@gnosis.pm/mock-contract": "^4.0.0",
    "@nomiclabs/buidler": "^1.4.4",
    "@nomiclabs/buidler-ethers": "^2.0.0",
    "@nomiclabs/buidler-truffle5": "^1.3.4",
    "@nomiclabs/buidler-web3": "^1.3.4",
    "@openzeppelin/test-helpers": "^0.5.6",
    "dotenv": "^8.2.0",
    "ganache-cli": "v6.10.1-beta.2",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.11",
    "solhint": "^3.2.0",
    "solidity-coverage": "^0.7.10"
  },
  "dependencies": {
    "@openzeppelin/contracts": "^3.0.1",
    "abdk-libraries-solidity": "^2.3.0",
    "solidity-fixedpoint": "^1.0.1",
    "solidity-fixedpoint": "^1.0.1",
    "ethers": "^5.0.3"
  }
}