{
  "name": "@apy-finance/apy-core",
  "version": "5.1.0",
  "description": "",
  "keywords": [
    "ethereum",
    "solidity",
    "defi"
  ],
  "files": [
    "contracts",
    "artifacts",
    "utils/constants.js",
    "deployed_addresses"
  ],
  "scripts": {
    "check": "yarn lint",
    "clean": "buidler clean",
    "compile": "buidler compile",
    "test:unit": "buidler test test-unit/*",
    "test:integration": "buidler test test-integration/APYPoolToken.js",
    "lint": "solhint contracts/**/*.sol contracts/*.sol",
    "coverage": "buidler coverage --network coverage --testfiles \"test-unit/*.js\"",
    "ganache": "ganache-cli",
    "fork:mainnet": "./scripts/fork_mainnet.sh",
    "deploy:kovan": "yarn buidler --network kovan run",
    "deploy:mainnet": "yarn buidler --network mainnet run"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "./git_hooks/commit-msg"
    }
  },
  "lint-staged": {
    "contracts/**/*.sol": "solhint -w 0",
    "*.js": "eslint --cache --fix",
    "*.{js,css,md}": "prettier --write"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/apy-finance/apy-core.git"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "author": "APY.Finance",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/apy-finance/apy-core/issues"
  },
  "homepage": "https://github.com/apy-finance/apy-core/README.md",
  "devDependencies": {
    "@gnosis.pm/mock-contract": "^4.0.0",
    "@nomiclabs/buidler": "^1.4.6",
    "@nomiclabs/buidler-ethers": "^2.0.0",
    "@nomiclabs/buidler-etherscan": "^2.1.0",
    "@nomiclabs/buidler-truffle5": "^1.3.4",
    "@nomiclabs/buidler-web3": "^1.3.4",
    "@openzeppelin/test-helpers": "^0.5.6",
    "dotenv": "^8.2.0",
    "eslint": "^7.11.0",
    "eslint-config-prettier": "^6.14.0",
    "ethers": "5.0.18",
    "ganache-cli": "^6.10.2",
    "ganache-time-traveler": "^1.0.15",
    "husky": "4.0.10",
    "lint-staged": "^10.2.11",
    "prettier": "2.1.2",
    "solhint": "^3.2.0",
    "solidity-coverage": "^0.7.10"
  },
  "dependencies": {
    "@chainlink/contracts": "^0.0.9",
    "@compound-finance/compound-js": "^0.1.0",
    "@openzeppelin/contracts": "^3.2.0",
    "@openzeppelin/contracts-ethereum-package": "^3.0.0",
    "abdk-libraries-solidity": "^2.3.0"
  }
}
